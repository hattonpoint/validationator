{
  "name": "validationator",
  "version": "2.0.2",
  "description": "A versitile data validator and static type checker. Creat complex models for deeply nested data structures. Create statically typed functions. Switch to bool mode of use in form validation, and much more.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/src",
    "test:dist": "./node_modules/mocha/bin/mocha test/dist",
    "test:prod": "./node_modules/mocha/bin/mocha test/prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "babel src --presets es2015-script --out-dir dist",
    "minify": "minify dist -d dist",
    "clean": "rm -rf dist",
    "publish:start": "chmod +x ./bin/publish-start.sh; ./bin/publish-start.sh",
    "publish:finish": "chmod +x ./bin/publish-finish.sh; ./bin/publish-finish.sh"
  },
  "repository": "https://github.com/hattonpoint/validationator.git",
  "keywords": [
    "validation",
    "validate",
    "type checker",
    "type",
    "checker",
    "front end",
    "backend",
    "testing",
    "typed",
    "typed functions",
    "typed",
    "deeply nested",
    "nested",
    "data structures",
    "data",
    "structures",
    "static",
    "validator",
    "validation",
    "validate"
  ],
  "author": "Ian Lancaster",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-es2015-script": "^1.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.1.0",
    "npm-cli-login": "0.0.10",
    "validationator": "^2.0.2"
  },
  "dependencies": {
    "validator": "^9.2.0"
  }
}
